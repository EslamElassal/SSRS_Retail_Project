  <DataSets>
    <DataSet Name="AdventureWorksDataSet">
      <Query>
        <DataSourceName>AdventureWorksDataSource</DataSourceName>
        <CommandText>SELECT Distinct 
  Kazyon.ProductCategoryID
  ,Kazyon.ProductCategoryName
  ,Kazyon.ProductSubcategoryID
  ,Kazyon.ProductSubcategoryName
  ,Kazyon.ProductID AS [Kazyon ProductID]
  ,Kazyon.Name as ProductName
  ,Kazyon.Cat#
  ,Kazyon.SubCat#
  ,Sales.SalesOrderDetail.ProductID AS [SalesOrderDetail ProductID]
  ,Sales.SalesOrderDetail.SalesOrderID AS [SalesOrderDetail SalesOrderID]
  ,Sales.SalesOrderDetail.SalesOrderDetailID
  ,Sales.SalesOrderHeader.SalesOrderID AS [SalesOrderHeader SalesOrderID]
  ,Sales.SalesOrderHeader.ShipDate
  ,IsNull(Sales.SalesOrderHeader.TotalDue,0) As TotalDue
, Month(ShipDate) as Month,Year(ShipDate) as Year 
FROM
  Sales.SalesOrderHeader
   inner JOIN Sales.SalesOrderDetail
    ON Sales.SalesOrderHeader.SalesOrderID = Sales.SalesOrderDetail.SalesOrderID
 inner JOIN Kazyon
    ON Kazyon.ProductID = Sales.SalesOrderDetail.ProductID
     where (Year(ShipDate) between 2012 and 2014 ) and (Month(ShipDate) between 1 and 3) order by Year(ShipDate),Month(ShipDate)</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <AutoJoin>False</AutoJoin>
            <SelectedColumns>
              <ColumnExpression ColumnOwner="Kazyon" ColumnName="ProductCategoryID" />
              <ColumnExpression ColumnOwner="Kazyon" ColumnName="ProductCategoryName" />
              <ColumnExpression ColumnOwner="Kazyon" ColumnName="ProductSubcategoryID" />
              <ColumnExpression ColumnOwner="Kazyon" ColumnName="ProductSubcategoryName" />
