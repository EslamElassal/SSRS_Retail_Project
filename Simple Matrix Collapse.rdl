  <DataSets>
    <DataSet Name="Films">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT
  Country.CountryID
  ,Country.Country
  ,Certificate.CertificateID
  ,Certificate.Certificate
  ,Genre.GenreID
  ,Genre.Genre
  ,Film.RunTimeMinutes
,Case Film.OscarWins when 0 Then 'Loser' Else 'Winner' End as Success
FROM
  Film
  INNER JOIN Certificate
    ON Film.CertificateID = Certificate.CertificateID
  INNER JOIN Country
    ON Film.CountryID = Country.CountryID
  INNER JOIN Genre
    ON Film.GenreID = Genre.GenreID</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
