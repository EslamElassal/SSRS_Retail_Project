              <ColumnExpression ColumnOwner="Country" ColumnName="Country" />
              <ColumnExpression ColumnOwner="Certificate" ColumnName="CertificateID" />
              <ColumnExpression ColumnOwner="Certificate" ColumnName="Certificate" />
              <ColumnExpression ColumnOwner="Genre" ColumnName="GenreID" />
              <ColumnExpression ColumnOwner="Genre" ColumnName="Genre" />
              <ColumnExpression ColumnOwner="Film" ColumnName="RunTimeMinutes" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Film" />
              <Join Type="INNER JOIN" ColumnOwner="Certificate">
                <On>
                  <From ColumnOwner="Film" ColumnName="CertificateID" />
                  <To ColumnOwner="Certificate" ColumnName="CertificateID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Country">
                <On>
                  <From ColumnOwner="Film" ColumnName="CountryID" />
