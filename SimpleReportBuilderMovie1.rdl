  <DataSets>
    <DataSet Name="MoviesData">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT
  Certificate.Certificate
  ,Country.Country
  ,Director.FirstName
  ,Film.OscarWins
  ,Genre.Genre
  ,Film.Title
  ,Film.ReleaseDate
  ,Film.RunTimeMinutes
  ,Film.OscarNominations
  ,Studio.Studio
FROM
  Film
  INNER JOIN Certificate
    ON Film.CertificateID = Certificate.CertificateID
  INNER JOIN Country
    ON Film.CountryID = Country.CountryID
  INNER JOIN Director
    ON Film.DirectorID = Director.DirectorID
  INNER JOIN Genre
    ON Film.GenreID = Genre.GenreID
  INNER JOIN Studio
    ON Film.StudioID = Studio.StudioID</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <AutoJoin>False</AutoJoin>
            <SelectedColumns>
              <ColumnExpression ColumnOwner="Certificate" ColumnName="Certificate" />
              <ColumnExpression ColumnOwner="Country" ColumnName="Country" />
              <ColumnExpression ColumnOwner="Director" ColumnName="FirstName" />
              <ColumnExpression ColumnOwner="Film" ColumnName="OscarWins" />
              <ColumnExpression ColumnOwner="Genre" ColumnName="Genre" />
              <ColumnExpression ColumnOwner="Film" ColumnName="Title" />
              <ColumnExpression ColumnOwner="Film" ColumnName="ReleaseDate" />
              <ColumnExpression ColumnOwner="Film" ColumnName="RunTimeMinutes" />
              <ColumnExpression ColumnOwner="Film" ColumnName="OscarNominations" />
              <ColumnExpression ColumnOwner="Studio" ColumnName="Studio" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Film" />
              <Join Type="INNER JOIN" ColumnOwner="Certificate">
                <On>
                  <From ColumnOwner="Film" ColumnName="CertificateID" />
                  <To ColumnOwner="Certificate" ColumnName="CertificateID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Country">
                <On>
                  <From ColumnOwner="Film" ColumnName="CountryID" />
                  <To ColumnOwner="Country" ColumnName="CountryID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Director">
                <On>
                  <From ColumnOwner="Film" ColumnName="DirectorID" />
                  <To ColumnOwner="Director" ColumnName="DirectorID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Genre">
                <On>
                  <From ColumnOwner="Film" ColumnName="GenreID" />
                  <To ColumnOwner="Genre" ColumnName="GenreID" />
                </On>
              </Join>
              <Join Type="INNER JOIN" ColumnOwner="Studio">
                <On>
                  <From ColumnOwner="Film" ColumnName="StudioID" />
                  <To ColumnOwner="Studio" ColumnName="StudioID" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
      </Query>
